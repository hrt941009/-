#import "SKUDoneTableViewCell.h"

@implementation SKUDoneTableViewCell

- (void)awakeFromNib {
    
    _priceLabel.textColor = [UIColor colorRGBWithRed:233.f green:60.f blue:103.f alpha:1.f];
    
    _doneButton.backgroundColor = [UIColor colorRGBWithRed:224.f green:30.f blue:84.f alpha:1.f];
    _doneButton.layer.cornerRadius = 5.f;
    [_doneButton setTitle:@"确认" forState:UIControlStateNormal];
    [_doneButton setTitleColor:[UIColor whiteColor] forState:UIControlStateNormal];
    _doneButton.titleLabel.font = [UIFont systemFontOfSize:15.f];
}

- (void)setSelected:(BOOL)selected animated:(BOOL)animated {
    [super setSelected:selected animated:animated];

    // Configure the view for the selected state
}

@end
