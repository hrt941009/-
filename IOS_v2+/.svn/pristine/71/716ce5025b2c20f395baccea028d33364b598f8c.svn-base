#import "NotificationCenterCell.h"

@implementation NotificationCenterCell

- (void)awakeFromNib {
    // Initialization code
    self.iconImageView.layer.borderColor = [[UIColor GitHub] CGColor];
    self.iconImageView.layer.borderWidth = 1.f;
    self.iconImageView.layer.masksToBounds = YES;
    self.iconImageView.layer.cornerRadius = 4.f;
    
    self.status.layer.masksToBounds = YES;
    self.status.layer.cornerRadius = 3.f;
    
    self.detail.numberOfLines = 0;
    
    
}

- (void)setSelected:(BOOL)selected animated:(BOOL)animated {
    [super setSelected:selected animated:animated];

    // Configure the view for the selected state
}

@end
